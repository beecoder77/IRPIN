<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abito.login">
    <uses-permission android:name="android.permission.INTERNET" />
    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="IRPIN"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".utamaActivity"
            android:label="MENU UTAMA" />
        <activity
            android:name=".ketinggianairActivity"
            android:label="KETINGGIAN AIR"
            android:parentActivityName=".utamaActivity" />
        <activity
            android:name=".kelembabanActivity"
            android:label="Kelembaban Tanah"
            android:parentActivityName=".utamaActivity" />
    </application>

</manifest>